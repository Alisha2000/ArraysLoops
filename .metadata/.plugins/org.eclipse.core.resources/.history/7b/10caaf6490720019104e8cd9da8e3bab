
public class Ex1 {
	
	
	public static void main(String[] args) {
		//3. Дан array {1,2,3,5,6,1234,123,312,231,9534, 1}
		//Задача: Посчитать среднее арифметическое
		//(сложить все элементы и поделить их на кол-во элементов)
		int arr[]=  {2, 7};
		int sum=0;
		for(int i=0; i<arr.length;i++) {
			sum+=arr[i];
			
		}System.out.println("Average is: "+sum/arr.length);
		
		

	}

}
