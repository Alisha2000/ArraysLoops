package StringBuilder;

import java.util.ArrayList;
import java.util.Arrays;
import java.util.Collections;
import java.util.List;


public class HomePr {
	public static List switchPairs(List myList) {
		List arr=Arrays.asList(myList);
		for(int a=0, i=1;i<myList.size();a+=2,i+=2) {
			arr.add(myList.get(i));
			arr.add(myList.get(a));
		}
		return arr;
		
		}
	
		public static void main(String[] args) {
			List n=Arrays.asList("a","b",2,5,6,7);
			System.out.println(switchPairs(n));
			
		}

	
	
}
