package StringBuilder;

public class SBMethods {
	public static void main(String[] args) {
		StringBuilder word1=new StringBuilder();
		word1.append("Java");
		word1.append(" Programming");
		System.out.println(word1);
		//Java - Programming
		//insert
		word1.insert(5,  "-");
		System.out.println(word1);
		word1.insert(6,  " ");
		System.out.println(word1);
	}

}
