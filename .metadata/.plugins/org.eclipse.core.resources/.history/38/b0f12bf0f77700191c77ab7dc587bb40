package myPractice;

public class settersAndGettersUdemy {
	private int height;
	private int weight;
	
	public void setHeight(int h) {
		height=h;
		System.out.println(height);
	}
	
	public int getHeight() {
		return height;
	}
	
	public void setWeight(int w) {
		weight=w;
	}
	public int getWeight() {
		return weight;
	}
	

}
