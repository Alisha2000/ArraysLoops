package examples88;

public class practiceIfStUdemy {
	public static void main(String[] args) {
		int x=5;
		int y=15;
		if (x>y) {
		System.out.println("x is bigger than y"); 
		}
		else if (y>x) {
			System.out.println("y is bigger than x");
		}
		else {
			System.out.println("x equals to y");
		}
		
	
		}
}
