
public class charOccurence {
	public static void main(String[] args) {
		String s="asdhnmk";
		char[] c=s.toCharArray();
	
		int count;
		int i=0,j=0;
		for(i=0; i<c.length; i++) {
			for(j=0;j<c.length;j++) {
				if(j<i&&c[i]==c[j]) {
					break;
				}
				if(c[j]==c[i]) {
					count++;
				}
			}
			System.out.println("the character"+c[i]+" is present "+count+" times");
		}
		
	}

}
