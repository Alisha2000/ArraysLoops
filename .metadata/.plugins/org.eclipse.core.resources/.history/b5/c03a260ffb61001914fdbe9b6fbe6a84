package examples88;
import java.util.Scanner;

public class playingWithLoop {
	public static void main(String[] args) {
		String dots="......";
		int num=1;
		int dotNum=4;
		for(; dotNum<0;   dotNum--, num++) {
			System.out.print(dots.substring(0,dotNum));
			System.out.println(num);
		}
		
		
		
	}
		
	
	
			 
		
		
}
