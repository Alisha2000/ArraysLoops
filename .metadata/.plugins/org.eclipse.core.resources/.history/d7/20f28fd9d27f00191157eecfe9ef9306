package Polymorphysm;

public class test {
	public static void main(String[] args) {
//reference side     object side
//super class		child class
		father f1=new child();
		f1.drive();
		f1.shave();
		((child)f1).sayHi();
		
		
		
		child f2=new grandChild();
		f2.drive();
		f2.sayHi();
		f2.shave();
		
		
		((grandChild)f2).cry();
		
			
		
	}

}
