		Scanner scanner=new Scanner(System.in);
		System.out.println("Available music types are Classical, Pop, Country.");
		System.out.println("Please enter the music type:");
		String input = scanner.next();
		
		String classical= "classical";
		String pop="pop";
		String country="country";
		String rap ="rap";
		String rock= "rock";
		if (input.equals(rock)) {
			System.out.print("rock is not available");
		}if(input.contentEquals(rap)) {
			System.out.println("rap is not available");
		}
		
		
				
				
		
		
			
		
			
				 
			
		
		
	}

}
