package myPractice;

public class settersAndGettersUdemy {
	private int height;
	private int weight;
	
	
	public void setHeight(int h) {
		if (h>0) {		
		height=h;
		}
	}
	
	public int getHeight() {
		return height;
	}
	
	public void setWeight(int w) {
		if (w>0) {
		weight=w;
		}
	}
	public int getWeight() {
		return weight;
	}
	
	public int getArea() {
		return height*weight;
	}

}
