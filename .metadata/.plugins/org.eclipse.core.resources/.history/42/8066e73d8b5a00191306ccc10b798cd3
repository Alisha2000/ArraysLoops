package examples88;

public class practiceIfStUdemy {
	public static void main(String[] args) {
		int x=5;
		int y=5;
		if (x>y) {
		System.out.println("x is bigger than y, "+(x-y)); 
		}
		else if (y>x) {
			System.out.println("y is bigger than x, "+(y-x));
		}
		else {
			System.out.println("x equals to y, "+(x-y) );
		}
		
	
		}
}
