package arrays;

import java.util.Arrays;

public class HomeWork2 {
	public static void main(String[] args) {
		int[]array=new int[25];
		for(int i=0; i<=25;i++) {
		array[i]=i;
		}System.out.println(Arrays.toString(array));
		int reverse=0;
		for(int i=array.length-1; i>=0; i--) {
			reverse=+array[i];
			System.out.print(reverse);
		}
		
	}

}
